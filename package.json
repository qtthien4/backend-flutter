{
  "name": "matching-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "yarn build && node dist/main",
    "add-migration": "yarn build && yarn run typeorm migration:generate -n",
    "database-update": "yarn build && yarn run typeorm migration:run",
    "create-migration": "yarn build && yarn run typeorm migration:create -n",
    "revert-migration": "yarn build && yarn run typeorm migration:revert ",
    "gen-mgrt": "npm run build && ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "run-mgrt": "npm run build && ts-node ./node_modules/typeorm/cli.js migration:run",
    "create-mgrt": "npm run build && ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "revert-mgrt": "npm run build && ts-node ./node_modules/typeorm/cli.js migration:revert",
    "gen-res": "nest g --no-spec resource",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.142.0",
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/platform-socket.io": "^8.4.5",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "8.0.3",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.23.0",
    "fs": "^0.0.1-security",
    "guid-typescript": "^1.0.9",
    "hbs": "^4.1.2",
    "json2csv": "^5.0.7",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.6.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "random-token": "^0.0.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.45",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/json2csv": "^5.0.3",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
